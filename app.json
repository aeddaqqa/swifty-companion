{
  "expo": {
    "name": "swift-companion",
    "scheme" : "com.farwila.swifty-companion",
    "slug": "swift-companion",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farwila.swifty-companion"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "io.expo.client"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}